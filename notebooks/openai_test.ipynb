    "# quick_test.py\n",
    "import os\n",
    "from openai import OpenAI\n",
    "client = OpenAI(api_key=os.getenv(\"OPENAI_API_KEY\"))\n",
    "resp = client.chat.completions.create(\n",
    "    model=\"gpt-4o-mini\",\n",
    "    messages=[{\"role\": \"user\", \"content\": \"Wie geht es dir?\"}],\n",
    ")\n",
    "print(resp.choices[0].message.content)\n"
   ]
  }
